                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    >


                    <Button
                        android:id="@+id/bDiv"
                        android:layout_height="0dp"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="/"
                        android:textSize="32sp"
                        android:gravity="center"
                        android:onClick="onOpsClick"
                        android:background="#ffb4bec8"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp"/>
                    <Button
                        android:id="@+id/bTimes"
                        android:layout_height="0dp"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="*"
                        android:textSize="32sp"
                        android:gravity="center"
                        android:onClick="onOpsClick"
                        android:background="#ffb4bec8"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp"/>
                    <Button
                        android:id="@+id/bMinus"
                        android:layout_height="0dp"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="-"
                        android:textSize="32sp"
                        android:gravity="center"
                        android:onClick="onOpsClick"
                        android:background="#ffb4bec8"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp"/>
                    <Button
                        android:id="@+id/bPlus"
                        android:layout_height="0dp"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="+"
                        android:textSize="32sp"
                        android:gravity="center"
                        android:onClick="onOpsClick"
                        android:background="#ffb4bec8"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp"/>


                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    >

                    <Button
                        android:id="@+id/bC"
                        android:layout_height="0dp"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="C"
                        android:textSize="32sp"
                        android:gravity="center"
                        android:onClick="onOpsClick"
                        android:background="#ffc8654c"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp"/>
                    <Button
                        android:id="@+id/bback"
                        android:layout_height="0dp"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="Del"
                        android:textSize="24sp"
                        android:gravity="center"
                        android:onClick="onOpsClick"
                        android:background="#ffb4bec8"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp"/>

                    <Button
                        android:id="@+id/bEnter"
                        android:layout_height="0dp"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="Enter"
                        android:textSize="24sp"
                        android:gravity="center"
                        android:onClick="onOpsClick"
                        android:background="#ffb4bec8"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp"/>
                    <Button
                        android:id="@+id/bEqu"
                        android:layout_height="0dp"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="="
                        android:textSize="32sp"
                        android:gravity="center"
                        android:onClick="onOpsClick"
                        android:background="#ffb4bec8"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp"/>
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
</LinearLayout>